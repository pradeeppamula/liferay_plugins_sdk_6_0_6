    <link rel="icon" href="/ieeecs-MyComputer-theme/images/favicon.ico">
</head>
<body>
    #if ($is_signed_in)
    #parse ("$full_templates_path/dockbar.vm")
    #end
	## Wrap all page content here
    <div id="wrap">
		## Begin page content
		<div class="container">
            #if ($selectable)
                $theme.include($content_include)
            #else
                $portletDisplay.recycle()
                $portletDisplay.setTitle($the_title)
                $theme.wrapPortlet("portlet.vm", $content_include)
            #end
		</div> ## /.container
	</div> ## /#wrap

    <div id="footer">
        <div class="container">
            <div class="col-md-4 col-sm-3 col-xs-1"></div>
            <div class="col-md-2 col-sm-3 col-xs-5 text-center">
                <a href="#"><img class="footer-logo" src="/ieeecs-MyComputer-theme/images/cs-logo.png"/></a>
            </div>
            <div class="col-md-2 col-sm-3 col-xs-5 text-center">
                <a href="#"><img class="footer-logo" src="/ieeecs-MyComputer-theme/images/ieee-logo.png"/></a>
            </div>
            <div class="col-md-4 col-sm-3 col-xs-1"></div>
        </div>
    </div>

    ## Le javascript
	##==================================================
	## Placed at the end of the document so the pages load faster
    <script src="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.0.2/js/bootstrap.min.js"></script>
</body>
$theme.include($bottom_include)
</html>