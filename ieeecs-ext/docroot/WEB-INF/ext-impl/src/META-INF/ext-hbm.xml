<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<class name="org.computer.portal.model.impl.ExtGroupImpl" table="ExtGroup">
		<cache usage="read-write" />
		<id name="groupId" type="long">
			<generator class="assigned" />
		</id>
		<property name="instantCommunity" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="keywords" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="modifierUserId" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="created" type="org.hibernate.type.TimestampType" />
		<property name="modified" type="org.hibernate.type.TimestampType" />
	</class>
	<class name="org.computer.portal.model.impl.ExtUserImpl" table="ExtUser">
		<cache usage="read-write" />
		<id name="userId" type="long">
			<generator class="assigned" />
		</id>
		<property name="csMember" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="staff" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="canCreateInstantCommunities" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="societies" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="publications" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
	<!-- class name="org.computer.portal.model.impl.UserAttributeImpl" table="UserAttribute">
		<cache usage="read-write" />
		<id name="userAttributeId" type="long">
			<generator class="assigned" />
		</id>
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="attributeType" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="attributeClass" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="attributeValue" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class-->	
</hibernate-mapping>